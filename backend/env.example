# Server
PORT=3000
NODE_ENV=development

# JWT Secret (gerar uma string aleatória segura)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# Database PostgreSQL
# Formato: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
# Exemplo local: postgresql://postgres:postgres@localhost:5432/sway_db
# Exemplo produção: postgresql://user:password@host:5432/sway_prod
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/sway_db

# CORS
CORS_ORIGIN=http://localhost:8000,https://swaybrasil.com

# Frontend URL
FRONTEND_URL=http://localhost:8000
